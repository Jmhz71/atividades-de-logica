   media <- nota1 + nota2 /2

limpatela
   
se(nota1 > 9) entao
escreva(" seu conceito é A, e você foi aprovado")
senao
escreva ("reprovado")
fimse

se (nota1 >= 7.5) entao
fimse

se (nota1 >= 6) entao
